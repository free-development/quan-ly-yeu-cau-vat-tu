<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="model.MucDich" table="MUCDICH">
		<id name="mdMa" type="java.lang.String" access="field">
			<column name="MDMA" />
			<generator class="assigned" />
		</id>
		<property name="mdTen" type="java.lang.String" access="field">
			<column name="MDTEN" />
		</property>
		<property name="daXoa" type="int" access="field"> 
 			<column name="DAXOA" /> 
 		</property>
	</class>
</hibernate-mapping>
