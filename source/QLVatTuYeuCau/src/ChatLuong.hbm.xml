<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.ChatLuong" table="CHATLUONG" catalog="vattu">
		<id name="clMa" type="java.lang.String">
			<column name="CLMA" />
			<generator class="assigned" />
		</id>
		<property name="clTen" type="java.lang.String">
			<column name="CLTEN" />
		</property>
		<property name="daXoa" type="int">
			<column name="DAXOA" />
		</property>
	</class>
</hibernate-mapping>
