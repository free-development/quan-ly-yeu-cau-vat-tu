<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.DonVi" table="DONVI" catalog="vattu">
		<id name="dvMa" type="java.lang.String">
			<column name="DVMA" />
			<generator class="assigned" />
		</id>
		<property name="dvTen" type="java.lang.String">
			<column name="DVTEN" />
		</property>
		<property name="sdt" type="java.lang.String">
			<column name="SDT" />
		</property>
		<property name="email" type="java.lang.String">
			<column name="EMAIL" />
		</property>
		<property name="diaChi" type="java.lang.String">
			<column name="DIACHI" />
		</property>
		<property name="daXoa" type="int">
			<column name="DAXOA" />
		</property>
	</class>
</hibernate-mapping>
