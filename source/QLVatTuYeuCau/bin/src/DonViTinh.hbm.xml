<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.DonViTinh" table="DONVITINH" catalog="vattu">
		<id name="dvtId" type="int" access="field">
			<column name="DVTID" />
			<generator class="increment" />
		</id>
		<property name="dvtTen" type="string">
			<column name="DVTTEN"></column>
		</property>
		<property name="daXoa" type="int" access="field"> 
 			<column name="DAXOA" /> 
 		</property>
	</class>
</hibernate-mapping>
