<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.ChucDanh" table="CHUCDANH" catalog="vattu">
		<id name="cdMa" type="java.lang.String">
			<column name="CDMA" />
			<generator class="assigned" />
		</id>
		<property name="cdTen" type="java.lang.String">
			<column name="CDTEN" />
		</property>
		<property name="daXoa" type="int" access="field"> 
 			<column name="DAXOA" /> 
 		</property>
	</class>
</hibernate-mapping>
