<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.VTCongVan" table="VTCONGVAN">
        <id name="cvId" type="int" access="field">
            <column name="CVID" />
            <generator class="assigned" />
        </id>
        <many-to-one name="vaiTro" class="model.VaiTro" access="field" fetch="join">
            <column name="VAITRO" />
        </many-to-one>
        <many-to-one name="nguoiDung" class="model.NguoiDung" access="field" fetch="join">
            <column name="NGUOIDUNG" />
        </many-to-one>
    </class>
</hibernate-mapping>
