<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="model.File" table="FILE" catalog="vattu">
		<id name="fileId" type="int">
			<column name="FILEID" />
			<generator class="increment" />
		</id>
		<property name="diaChi" type="java.lang.String">
			<column name="DIACHI" />
		</property>
		<property name="moTa" type="java.lang.String">
			<column name="MOTA" />
		</property>
		<property name="cvId" type="int">
			<column name="CVID" />
		</property>
	</class>
</hibernate-mapping>
